

/* Requires the Docker Pipeline plugin */
pipeline {
    agent any
    stages {
        stage('build init') {
            steps {
                sh 'pwd && ls && cd django/init && docker build --tag Django_db_init:latest . '
            }
        }
        stage('build app') {
            steps {
                sh 'cd django/app && docker build --tag Django_app:latest . '

            }
        }
    }
}
